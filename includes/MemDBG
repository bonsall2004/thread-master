#pragma once
#include <cstddef>
#include <cstdlib>
#include <cstdio>
#include <vector>

void* operator new(size_t size)
{
  void* ptr = malloc(size);
  if(ptr == nullptr)
  {
    printf("Failed to Allocate: %zu \n", size);
  }
  else
  {
    printf("%d: Memory Allocated: %zu \n", 0, size);
  }
  return ptr;
}

void operator delete(void* ptr, size_t size) noexcept
{
  free(ptr);
  printf("Memory Freed: %zu\n", size);
}